#Q40)
def keywrk():
    d1={'arnav':2,'ok':5,'stock':4}
    c=1
    for i in d1:
        c*=d1[i]
    print('sum of values=',c)
#Q41
def key():
    d={"Arnav":4,"Chips":2,"PS4":8,"Arnav":4,"PS4":8}

    for i in d:
        for j in i:
            if j==i:
                d.pop(i)
    print(d)
        
#Q52
def work():
    d={}
    w1=int(input("How many monument + location you want to enter: "))
    for i in range(w1):
        x=input("Enter monument: ")
        w=input("Enter location: ")
        d[x]=w
    p=input("Enter the monument which you want to search for: ")
    f=0
    for j in d:
        if j==p:
            f=1
    if f==1:
        print("Yes its present.")
    else:
        print("Not its not present.")
        
#Q49
def power():
    d={}
    w1=int(input("Enter how many entries: "))
    for i in range(w1):
        w=input("Enter name")
        x=int(input("Enter salary"))
        d[w]=x
    print(d)
    c=0
    x1=input("Enter the name to be searched for: ")
    for i in d:
        if x1==i:
            c=1
    if c==1:
        print("Yes its present. Salary=",d[x1])
    else:
        print("No its not present.")



