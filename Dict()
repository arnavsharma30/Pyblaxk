def arnav(ch1):
    s=0
    i=0
    while True:
        p=int(input("Enter number"))
        s+=p
        i+=1
        ch1=input("continue(y/n)")  
        if ch1=="y":
            continue
        else:
            break
    print("i=",i)
    av=s/i
    print("sum=",s)
    print("average=",av)


