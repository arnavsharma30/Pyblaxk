def ix():
    w={}
    x=int(input("How many entries"))
    for i in range(x):
        wp=input("Enter key:")
        wv=input("Enter value:")
        w[wp]=wv
    
    
    c=0
    for i in w:
        if i=={}:
            c=1
    if c==1:
        print("no data in dict")
        
    else: 
        print("data in dict")


def oo():
    dic1 = {1:10,2:20}
    dic2 = {3:30,4:40}
    dic3 = {5:50,6:60}
    
    print(dic1 | dic2 | dic3)

#input name of n employees and their salary details like 
#basic salary , house rent, convence allowence. Calculate total salary of each employee and dislay
def pr():
    w={}
    q=int(input("Enter how many entries: "))
    for i in range(q):
        name=input("Enter name: ")
        bs=int(input("Enter basic salary: "))
        hr=int(input("Enter house rent: "))
        ca=int(input("Enter Conveynce allowence: "))
        w[name]=(bs,hr,ca)
    print("name\t\t\ttotal salary")
    for i in w:
        s=0
        x=w[i]
        for j in x:
            s+=j
        print(i,"\t\t\t\t",s)




#Input a number and convert a number into words
def Entrepreneurship():
    w={0:"Zero",1:"One",2:"Two",3:"Threee",4:"Four",5:"Five",6:"Six",7:"Seven",8:"Eight",9:"Nine"}
    p=input("Enter the number which you want in words: ")
    for j in p:
        x=int(j)
        v=w[x]
        print(w[x],end=' ')
         
            
#Q41
def malfu():

    w = {"Hello":34, "Hi":5, "Hello":34, "Rishit":22,"Hi":5}
    d={}
    for i in w:
        if i not in d:
            d[i]=w[i]
    print(d)

#Q51
def pylib():
    w={}
    y=int(input("How many classes and names: "))
    for i in range(y):
        name=input("Enter name: ")
        clas=int(input("Enter class: "))
        w[name]=clas
    
    dele=input("Enter the name whcih you want to delete: ")
    for i in w:
        if dele in w:
            w.pop(dele)
        break
            
    print(w)



#Q52

def cisco():
    w={}
    y=int(input("How many Monuments: "))
    for i in range(y):
        name=input("Enter Monument: ")
        loc=input("Enter location of the Monument: ")
        w[name]=loc

    w1=input("Enter which monument you want to check if its present: ")
    h=0
    for i in w:
        if w1==i:
            h=1
    if h==1:
        print("Yes, this monument is present in the dict()")
    else:
        print("No :( its not present in the list")
        

#WAP store student info admission no., roll no., name,marks in a dict() and display information on the basis of admission no
#in a tabular format

def student():
    w=int(input("Enter how many entries: "))
    record={}
    for i in range(w):
        ad=int(input("Enter admission number: "))
        roll=int(input("Enter roll numer: "))
        marks=int(input("Enter marks: "))
        name=input("Enter name: ")
        record[ad]=(roll,marks,name)

    for i in record:
        print("Admission number:\t",i)
        x=record[i]
        print("roll no:\t\tmarks\t\tname")
        for j in x:
            print(j,end='\t\t')
        print()

#WAP to count the no of times a character appears in a given string using a dict()


def lateral():
    w=str(input("Enter string: "))
    k={}
    for i in w:
        if i in k:
            continue
        else:
            k[i]=w.count(i)
    print(k)


