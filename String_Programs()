    def ok():
        w=input("Enter words")
        o=w.split()
        for i in o:
            print(max(len(i)))


def we():

    #Print sum of the digits of a number
    w=int(input("Enter number"))
    s=0
    while(w>0):
        c=w%10
        s+=c
        w=w//10
    print("sum of the digits=",s)


    #print armstrong 
    w=int(input("Enter numeber"))
    z=w
    l=0
    while(w>0):
        y=w%10
        w1=y**3
        l+=w1
        w=w//10
    if l==z:
        print("It is an armstrong number")
    else:
        print("Ok")


def up():
    #WAP to uppercaase and lowercase the oppoite letters

    a="ArNAv"
    for i in range(len(a)):
        if i%2==0:
            print(a[i].upper(),end='')
        else:
            print(a[i].lower(),end='')


def perfect_Number():
    #program to find perfect number
    w=int(input("Enter number"))
    s=0
    for i in range(1,w):
        if w%i==0:
            s+=i
    if w==s:
        print("perfect no")
    else:
        print("not")

    #while conversion
    w=int(input("Enter number"))
    s=0
    i=1
    while(i<w):
        if w%i==0:
            s+=i
        i+=1
    if w==s:
        print("perfect no")
    else:
        print("not")



def neg():
    #WAP to enter 5 numbers count the number of negetive nad pos numbers
    se=0
    w=0
    for i in range(5):
        y=int(input("Enter number"))
        if y<0:
            se+=1
        else:
            w+=1
    print("Count of negetive numbers: ",se)
    print("count of pos number: ",w)


def tw():

    #WAP to read a number if its even print it's factors otherwise, factorial

    w=int(input("Enter a number"))
    if w%2==0:
        for io in range(1,w+1):
            if w%io==0:
                print(io)
    else:
        l=1
        for ip in range(1,w+1):
            l=l*ip
        print(l)

            